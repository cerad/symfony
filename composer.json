{
  "name": "cerad/symfony",
  "type": "library",
  "description": "My Symfony Tweaks",
  "license": "MIT",

  "require": {
    "php": ">=7.4.4",
    "symfony/routing": "^5.1",
    "symfony/config": "^5.1",
    "symfony/http-foundation": "^5.1",
    "symfony/dependency-injection": "^5.1",
    "symfony/http-kernel": "^5.1",
    "twig/twig": "^3.0"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/App/",
      "Cerad\\": "src/Cerad/"
    },
    "exclude-from-classmap": [
      "/vendor/cerad/symfony/src/App/"
    ]
  },

  "minimum-stability": "dev",
  "prefer-stable": true
}
